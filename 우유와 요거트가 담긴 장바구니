//https://school.programmers.co.kr/learn/courses/30/lessons/62284?language=oracle

MySQL

SELECT CART_ID
FROM CART_PRODUCTS
WHERE NAME IN('Milk', 'Yogurt')
GROUP BY CART_ID
HAVING COUNT(DISTINCT NAME) >= 2
ORDER BY CART_ID


Oracle

SELECT CART_ID
FROM CART_PRODUCTS
WHERE NAME IN('Milk', 'Yogurt')
GROUP BY CART_ID
HAVING COUNT(DISTINCT NAME) >= 2
ORDER BY CART_ID
