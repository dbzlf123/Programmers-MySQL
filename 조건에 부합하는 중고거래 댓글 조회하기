//https://school.programmers.co.kr/learn/courses/30/lessons/164673?language=oracle



MySQL

SELECT a.TITLE, a.BOARD_ID, b.REPLY_ID, b.WRITER_ID, b.CONTENTS, DATE_FORMAT(b.CREATED_DATE, '%Y-%m-%d') AS CRAETED_DATE
FROM USED_GOODS_BOARD a JOIN USED_GOODS_REPLY  b ON a.BOARD_ID = b.BOARD_ID
WHERE a.CREATED_DATE LIKE('2022-10%')
ORDER BY b.CREATED_DATE ASC, a.TITLE ASC


Oracle

SELECT a.TITLE, a.BOARD_ID, b.REPLY_ID, b.WRITER_ID, b.CONTENTS, TO_CHAR(b.CREATED_DATE, 'YYYY-MM-DD') AS CRAETED_DATE
FROM USED_GOODS_BOARD a JOIN USED_GOODS_REPLY  b ON a.BOARD_ID = b.BOARD_ID
WHERE TO_CHAR(a.CREATED_DATE, 'YYYY-MM-DD') LIKE '2022-10%'
ORDER BY b.CREATED_DATE ASC, a.TITLE ASC
